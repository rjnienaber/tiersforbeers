{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-return-await": "off",
    "max-len": "off",
    "object-curly-newline": "off",
    "arrow-body-style": "off",
    "operator-linebreak": "off"
  }
}
